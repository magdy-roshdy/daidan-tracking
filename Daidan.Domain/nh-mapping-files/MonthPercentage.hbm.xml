<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Daidan.Entities"
                   namespace="Daidan.Entities">
	<class name="MonthPercentage" table="MonthPercentage">

		<id name="Id" column="MonthPercentageId">
			<generator class="identity"/>
		</id>
		<property name="Month" column="MonthPercentageMonth" />
		<property name="Year" column="MonthPercentageYear" />
		<property name="Amount" column="MonthPercentageAmount" />

		<bag name="CustomersPercentage" lazy="false">
			<key column="CustomerPercentageMonthId" />
			<one-to-many class="CustomerPercentage"/>
		</bag>
	</class>
</hibernate-mapping>