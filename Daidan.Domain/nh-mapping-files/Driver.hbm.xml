<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Daidan.Entities"
                   namespace="Daidan.Entities">
	<class name="Driver" table="Drivers">

		<id name="Id" column="DriverId">
			<generator class="identity"/>
		</id>
		<property name="Name" column="DriverName" />
		<property name="IsActive" column="DriverIsActive" />
		<property name="IsOutsourced" column="DriverIsOutsourced" />
	</class>

</hibernate-mapping>