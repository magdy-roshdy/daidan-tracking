<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Daidan.Entities"
                   namespace="Daidan.Entities">
	<class name="MonthAminPercentage" table="MonthAminPercentage">

		<id name="Id" column="MonthPercentageId">
			<generator class="identity"/>
		</id>
		<property name="Month" column="MonthPercentageMonth" />
		<property name="Year" column="MonthPercentageYear" />
		<property name="Amount" column="MonthPercentageAmount" />
	</class>
</hibernate-mapping>